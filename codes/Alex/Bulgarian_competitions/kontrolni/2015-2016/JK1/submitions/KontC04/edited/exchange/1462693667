#include<iostream>
#include<algorithm>
using namespace std;
pair<unsigned long long,unsigned long long> ab[201];
pair<unsigned long long,unsigned long long> how[2001],how2[2001];

int main(){
    unsigned long long s,n;
    cin>>s>>n;
    for(unsigned long long i=0;i<n;i++){
        cin>>ab[i].first;
    }
    for(unsigned long long i=0;i<n;i++){
        cin>>ab[i].second;
    }
    sort(ab,ab+n);
    unsigned long long pov=0;
    for(unsigned long long i=0;i<n;i++){
        if(ab[i].first==ab[i+1].first){
            ab[i+1].second+=ab[i].second;
            ab[i].first=2001;
            pov++;
        }
    }
    sort(ab,ab+n);
    n-=pov;
    how[0].first=1;
    how[0].second=0;
    /*for(unsigned long long i=0;i<n;i++){
        cout<<ab[i].first<<" "<<ab[i].second<<endl;
    }*/
    for(unsigned long long i=0;i<n;i++){
            for(unsigned long long l=0;l<=s;l++){
                how2[l]=how[l];
            }
        for(unsigned long long j=1;j<=ab[i].second;j++){
            //cout<<ab[i].first<<"*"<<j<<endl;
            for(unsigned long long k=0;k+ab[i].first*j<=s;k++){
                if(how2[k].second!=ab[i].first && how2[k].first>0){
                    how[k+ab[i].first*j].first+=how2[k].first;
                    how[k+ab[i].first*j].second=ab[i].first;
                }
            }
            /*for(unsigned long long l=0;l<=s;l++){
                cout<<how[l].first<<" ";
            }*/
            //cout<<endl;
        }
    }
    cout<<how[s].first;
}
/*50
3
10 20 10
1 2 2*/
/*50
3
20 10 5
10 10 10*/
