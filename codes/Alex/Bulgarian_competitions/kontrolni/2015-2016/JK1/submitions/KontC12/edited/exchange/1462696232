#include <iostream>
#include <algorithm>
#define k first
#define br second
using namespace std;
int a[201],num[201],s,n,br=0,maxx[201];
pair <int,int> inv[202];
void rac(int sum,int pos)
{
 int i,ts;
 //cout <<sum<<" "<<pos<< endl;
 if(sum>s) return;
 for (i=0; i<=inv[pos].br; i++)
     {
      ts=inv[pos].k*i;
      if(sum+ts>s) return;
      if(pos!=0 && maxx[pos-1]+sum+ts>=s) rac(sum+ts,pos-1);
      if(pos==0)
         {
          if(s-sum%inv[0].k==0 && s-sum<=maxx[0]) br++;
          return;
         }
     }
}

int main()
{
int b,i;
cin >>s>>n;
for (i=1; i<=n; i++) cin>>a[i];
for (i=1; i<=n; i++)
    {
     cin>>b;
     num[a[i]]+=b;
    }
n=0;
for (i=1; i<=200; i++) if(num[i]>0) {inv[n].k=i; inv[n++].br=num[i];}
//for (i=0; i<n; i++) cout <<inv[i].k<<" "<<inv[i].br<< endl;
sort(inv,inv+n);
maxx[0]=inv[0].k*inv[0].br;
for (i=1; i<=n-1; i++) maxx[i]=maxx[i-1]+inv[i].k*inv[i].br;
rac(0,n-1);
cout <<br<< endl;
return 0;
}
/*
50
3
10 20 10
1 2 2
*/
