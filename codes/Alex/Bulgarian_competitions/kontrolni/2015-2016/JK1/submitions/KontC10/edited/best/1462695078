#include<iostream>
using namespace std;
int main(){
double a,b,otg;
long long n,m,k1,k2,k3,otg2;
char k[32];
cin>>n>>m;
cin>>a>>k1;
cin>>b>>k2;
if(k1+k2>n){
    if(a<b){
           // k3=k2;
        k2=n-k1;
    if(k2<0){
        k1=k1+k2;
    }
    }else{
     //   k3=k1;
    k1=n-k2;
        if(k1<0){
        k2=k2+k1;
    }
    }
}
/*for(long long i=0;i<m;i++){
cin>>k>>k;
}*/
otg=a*k1+b*k2;
otg2=otg*1000;
if((otg2%10)>=5){
        if((otg2/10)%100+1>=10){
cout<<otg2/1000<<'.'<<(otg2/10)%100+1;
        }else{
        cout<<otg2/1000<<".0"<<(otg2/10)%100+1;
        }
}else{
            if((otg2/10)%100>=10){
cout<<otg2/1000<<'.'<<(otg2/10)%100;
            }else{
            cout<<otg2/1000<<".0"<<(otg2/10)%100;
            }
}
cout<<"\n";
//cout<<otg2;
return 0;
}
