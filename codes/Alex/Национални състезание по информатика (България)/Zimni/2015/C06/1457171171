S'/**#include<iostream>\r\n#include<vector>\r\n#include<queue>\r\n#define endl \'\\n\'\r\nusing namespace std;\r\n\r\n#define NMax 100000\r\n\r\nstruct info\r\n{\r\n    int fi,se,tr,fo;\r\n\r\n    info(){};\r\n    info(int a,int b,int c,int d)\r\n    {\r\n        fi=a,se=b,tr=c,fo=d;\r\n    }\r\n};\r\n\r\nstruct triple\r\n{\r\n    int hexa,rot_tmp,rot_last;\r\n\r\n    triple(){};\r\n    triple(int a,int b,int c)\r\n    {\r\n        hexa=a,rot_tmp=b,rot_last=c;\r\n    }\r\n};\r\n\r\nint n,ans=-1;\r\nint a1,a2,a3,a4,a5,a6;\r\nint b1,b2,b3,b4,b5,b6;\r\nvector<triple> nb[NMax+5];\r\n\r\nvoid bfs(int s)\r\n{\r\n    queue<info> q;\r\n    q.push(info(s,0,0,0));\r\n    q.push(info(s,0,0,1));\r\n    q.push(info(s,0,0,2));\r\n    q.push(info(s,0,0,3));\r\n    q.push(info(s,0,0,-1));\r\n    q.push(info(s,0,0,-2));\r\n\r\n    while(!q.empty())\r\n    {\r\n        int tmp=q.front().fi,wave=q.front().se,minrot=q.front().tr,currot=q.front().fo;\r\n        q.pop();\r\n\r\n        if(tmp==n-1&&minrot>ans)\r\n        {\r\n            ans=minrot;\r\n            continue;\r\n        }\r\n\r\n        bool in=0;\r\n        for(int i=0;i<nb[tmp].size();i++)\r\n            if(nb[tmp][i].rot_last==currot)\r\n            {\r\n                q.push(info(tmp+1,wave+1,minrot+nb[tmp][i].rot_tmp,nb[tmp][i].rot_tmp));\r\n                in=1;\r\n            }\r\n        if(in==0)\r\n        {\r\n            cout<<"NO "<<wave<<endl;\r\n            return;\r\n        }\r\n    }\r\n}\r\n\r\nint main()\r\n{\r\n    ios_base::sync_with_stdio(0);\r\n    cin.tie(0);\r\n    cout.tie(0);\r\n\r\n    cin>>n;\r\n\r\n    for(int i=0;i<n;i++)\r\n    {\r\n        cin>>a1>>a2>>a3>>a4>>a5>>a6;\r\n\r\n        if(i>0)\r\n        {\r\n            if(a1==b4)\r\n            {\r\n                ///nb[i].push_back(triple(i-1,0,0));\r\n                nb[i-1].push_back(triple(i,0,0));\r\n            }\r\n            if(a1==b3)\r\n            {\r\n                ///nb[i].push_back(triple(i-1,0,-1));\r\n                nb