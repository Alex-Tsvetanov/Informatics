ack(triple(i-1,2,-1));\r\n                nb[i-1].push_back(triple(i,-1,2));\r\n            }\r\n            if(a3==b2)\r\n            {\r\n                ///nb[i].push_back(triple(i-1,2,-2));\r\n                nb[i-1].push_back(triple(i,-2,2));\r\n            }\r\n            if(a3==b1)\r\n            {\r\n                ///nb[i].push_back(triple(i-1,2,3));\r\n                nb[i-1].push_back(triple(i,3,2));\r\n            }\r\n            if(a3==b5)\r\n            {\r\n                ///nb[i].push_back(triple(i-1,2,2));\r\n                nb[i-1].push_back(triple(i,2,2));\r\n            }\r\n            if(a3==b6)\r\n            {\r\n                ///nb[i].push_back(triple(i-1,2,1));\r\n                nb[i-1].push_back(triple(i,1,2));\r\n            }\r\n\r\n            if(a4==b4)\r\n            {\r\n                ///nb[i].push_back(triple(i-1,3,0));\r\n                nb[i-1].push_back(triple(i,0,3));\r\n            }\r\n            if(a4==b3)\r\n            {\r\n                ///nb[i].push_back(triple(i-1,3,-1));\r\n                nb[i-1].push_back(triple(i,-1,3));\r\n            }\r\n            if(a4==b2)\r\n            {\r\n                ///nb[i].push_back(triple(i-1,3,-2));\r\n                nb[i-1].push_back(triple(i,-2,3));\r\n            }\r\n            if(a4==b1)\r\n            {\r\n                ///nb[i].push_back(triple(i-1,3,3));\r\n                nb[i-1].push_back(triple(i,3,3));\r\n            }\r\n            if(a4==b5)\r\n            {\r\n                ///nb[i].push_back(triple(i-1,3,2));\r\n                nb[i-1].push_back(triple(i,2,3));\r\n            }\r\n            if(a4==b6)\r\n            {\r\n                ///nb[i].push_back(triple(i-1,3,1));\r\n                nb[i-1].push_back(triple(i,1,3));\r\n            }\r\n\r\n            if(a5==b4)\r\n            {\r\n                ///nb[i].push_back(triple(i-1,-2,0));\r\n                nb[i-1].push_back(triple(i,0,-2));\r\n            }\r\n            if(a5==b3)\r\n  